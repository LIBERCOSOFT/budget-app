<h1>Create a new Category</h1>

<%= form_with model: @group, url: groups_path do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :icon %><br>
    <%= form.text_field :icon %>
  </div>
  <div>
    <%= form.submit "Create Post" %>
  </div>
<% end %>